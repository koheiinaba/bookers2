<%= render 'books/list', book: @book, user: @user %>

    <div class="col-xs-9">
      <h2>Users</h2>
      <table class="table table-hover teble-inverse">
        <thead>
          <tr>
            <th>image</th>
            <th>name</th>
            <th></th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <tr>
          <% @users.each do |user| %>
            <td><%= attachment_image_tag user, :profile_image, size:'50x50', format: 'jpeg', fallback: "no_image.jpg" %></td>
            <td><%= user.name %></td>
            <td><%= link_to "Show", user_path(user) %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>